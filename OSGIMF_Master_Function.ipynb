    "    Bl_stars_ejected = np.random.choice(m_Bl_star_array, N_ej_Bl_stars, replace = False)\n",
    "    A_stars_ejected = np.random.choice(m_A_star_array, N_ej_A_stars, replace = False)\n",
    "    #print(O_stars_ejected, Be_stars_ejected, Bl_stars_ejected, A_stars_ejected)\n",
    "\n",
    "    m_O_star_array_new = np.setdiff1d(m_O_star_array, O_stars_ejected)\n",
    "    m_Be_star_array_new = np.setdiff1d(m_Be_star_array, Be_stars_ejected)\n",
    "    m_Bl_star_array_new = np.setdiff1d(m_Bl_star_array, Bl_stars_ejected)\n",
    "    m_A_star_array_new = np.setdiff1d(m_A_star_array, A_stars_ejected)\n",
    "        \n",
    "    m_stars_ejected = np.concatenate((O_stars_ejected, Be_stars_ejected, Bl_stars_ejected, A_stars_ejected))\n",
    "    #print(m_stars_ejected)\n",
    "\n",
    "    m_stars_array_new = np.concatenate((m_O_star_array_new, m_Be_star_array_new, m_Bl_star_array_new, m_A_star_array_new, m_else_star_array))\n",
    "    return m_stars_array_new, m_stars_ejected\n",
    "\n",
    "from scipy.optimize import root_scalar\n",
    "\n",
    "def MLE_with_std(data):\n",
    "\n",
    "    Y=np.min(data)\n",
    "    Z=np.max(data)\n",
    "    T=np.sum(np.log(data))\n",
    "    n=len(data)\n",
    "\n",
    "    ## Does this really determine the exponent I look for?\n",
    "    def Determinator(alpha_ML):  \n",
    "        eq1 = -n/(1-alpha_ML)\n",
    "        eq2 = n*(Z**(1-alpha_ML)*np.log(Z)-Y**(1-alpha_ML)*np.log(Y))/(Z**(1-alpha_ML)-Y**(1-alpha_ML))-T\n",
    "        return eq1+eq2\n",
    "        \n",
    "    sol = root_scalar(Determinator, bracket=[-30.0, 0.999*30.0])\n",
    "    slope = sol.root\n",
    "    slope_unbiased=(n)/(n-2)*(slope-1)+1 ## According to Maschberger& Kroupa (2009)\n",
    "\n",
    "    \n",
    "    Fisher_information=-n*(-slope_unbiased/(1-slope_unbiased)**2+(slope_unbiased-1)*Y**(-slope_unbiased)*Z**(-slope_unbiased)*(Z-Y)*(np.log(Z)-np.log(Y))/(Z**(1-slope_unbiased)-Y**(1-slope_unbiased))**2)\n",
    "\n",
    "    Variance=1/(n*Fisher_information)\n",
    "    SE=np.sqrt(Variance) ### This is the standard error!! Not the standard deviation!\n",
    "    std=np.sqrt(1/Fisher_information)\n",
    "\n",
    "    return slope_unbiased, std\n",
    "\n",
    "def Stars_Cluster_n(n, mass_stars_array,num_stars_array):\n",
    "    return mass_stars_array[int(np.sum(num_stars_array[:n-1])):int(np.sum(num_stars_array[:n]))]"
